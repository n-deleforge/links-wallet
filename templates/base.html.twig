<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}me.bio{% endblock %}
		</title>
	
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>

		<header class="p-3 mb-3 border-bottom">
			<div class="container">
				<div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
					<ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
						<li>
							<a href="{{ path('app_home') }}" class="nav-link px-2 link-secondary">Home</a>
						</li>
						<li>
							<a href="#" class="nav-link px-2 link-dark">Contact</a>
						</li>
					</ul>

                    <div class="col-md-3 text-end">
                        <a href="{{ path('app_login') }}" class="btn" role="button">Login</a>
                        <a href="#" class="btn btn-dark" role="button">Sign-up</a>
                    </div>
				</div>
			</div>
		</header>

		{% block body %}{% endblock %}
	</body>
</html>
